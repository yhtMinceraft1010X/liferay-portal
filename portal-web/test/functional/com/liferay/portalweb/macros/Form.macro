	macro changeFieldTypeModalConfirmation {
		FormFields.changeFieldType(selectOption = "${selectOption}");

		while (IsElementPresent(locator1 = "Form#VALIDATE_CHANGE_FIELD_TYPE_MODAL_MESSAGE")) {
