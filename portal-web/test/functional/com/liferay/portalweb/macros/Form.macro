	macro addFieldToFieldsGroup {
		DragAndDrop.javaScriptDragAndDropToBottom(
			key_fieldLabel = "${targetFieldLabel}",
			key_fieldName = "${sourceFieldName}",
			locator1 = "Form#PAGE_FIELD_LABEL",
			locator2 = "FormFields#LABEL");
