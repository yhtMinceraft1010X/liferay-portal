	macro submitFormPublish {
		Form.viewFormPublish(
			fieldLabel = "${fieldLabel}",
			fieldName = "${fieldName}");

		FormFields.editText(
			fieldLabel = "${fieldLabel}",
			fieldName = "${fieldName}",
