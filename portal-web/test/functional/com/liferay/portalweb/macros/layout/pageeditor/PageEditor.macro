		}
		else {
			ContentPages.viewFragmentText(
				fragmentName = "${fragmentName}",
				id = "${id}",
				position = "${position}",
				text = "${editableText}");

			if (isSet(style)) {
				AssertVisible(
					id = "${id}",
					key_position = "${position}",
					key_style = "${style}",
					key_text = "${editableText}",
					locator1 = "Fragment#CONTRIBUTED_FRAGMENT_INLINE_RICH_TEXT_STYLE");
			}
		}
	}

