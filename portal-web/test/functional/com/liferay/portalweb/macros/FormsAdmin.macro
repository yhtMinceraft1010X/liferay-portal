	macro viewUploadFile {
		FormFields.viewUploadLabel(
			fieldLabel = "${fieldLabel}",
			fieldName = "${fieldName}");

		var key_fieldName = "${fieldName}";
		var key_uploadFileName = "${uploadFileName}";

		AssertTextEquals(
			locator1 = "FormFields#UPLOAD_FILE_NAME",
