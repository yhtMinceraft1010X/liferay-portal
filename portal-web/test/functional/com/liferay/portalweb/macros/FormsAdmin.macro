	}

	macro viewNoDuplicateForms {
		var key_formName = "${formName}";

		AssertElementNotPresent(locator1 = "FormsAdmin#FORMS_TABLE_NAME_2");
	}

	macro viewNoFormEntries {
		AssertTextEquals(
			locator1 = "Message#EMPTY_INFO",
