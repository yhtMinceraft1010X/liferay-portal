	macro viewPreview {
		AssertElementPresent(
			key_footer = "${footer}",
			locator1 = "ItemSelector#VIEWER_FOOTER");

		AssertElementPresent(
			key_imageFileName = "${imageFileName}",
			locator1 = "ItemSelector#VIEWER_FOOTER_IMAGE");
