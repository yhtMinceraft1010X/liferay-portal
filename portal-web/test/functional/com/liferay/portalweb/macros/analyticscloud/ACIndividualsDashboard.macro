		}

		AssertTextEquals.assertPartialText(
			index = "${index}",
			locator1 = "ACIndividualsDashboard#BREAKDOWN_RECHARTS_CARTESIAN_Y_AXIS_TICK",
			value1 = "${tick}");

		AssertAttributeValue(
			attribute1 = "fill",
