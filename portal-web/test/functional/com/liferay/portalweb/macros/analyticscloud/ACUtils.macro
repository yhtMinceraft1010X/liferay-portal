	macro getIPCountry {
		var curl = '''https://ipinfo.io''';
		var code = JSONCurlUtil.get("${curl}", "$.country");

		if ("${code}" == "GB") {
			var country = "United Kingdom";
		}

		if ("${code}" == "BR") {
			var country = "Brazil";
		}

		if ("${code}" == "US") {
			var country = "United States";
		}

		if ("${code}" == "CN") {
			var country = "China";
		}

		if ("${code}" == "ES") {
			var country = "Spain";
		}

		if ("${code}" == "NZ") {
			var country = "New Zealand";
		}

		return "${country}";
	}

