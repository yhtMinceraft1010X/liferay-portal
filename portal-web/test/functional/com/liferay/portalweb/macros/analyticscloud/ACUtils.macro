		var postDataText = ProxyUtil.getHarRecording("$..[?(@['pageref'] == '${pageRef}')].request[?(@.method == 'POST' && @.url contains '/${pageRef}')]..text");
		var value = JSONUtil.getWithJSONPath("[${postDataText}]", "$..${property}");

