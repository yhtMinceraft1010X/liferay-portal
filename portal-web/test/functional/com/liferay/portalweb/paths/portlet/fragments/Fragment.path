</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_BACKGROUND_COLOR</td>
	<td>//div[contains(@style,'background-color: var(--${key_paletteItem})')]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_BORDER_COLOR</td>
	<td>//div[contains(@style,'border-color: var(--${key_paletteItem})')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_BORDER_RADIUS</td>
	<td>//div[contains(@style,'border-radius: var(--${key_borderRadius})')][.//*[contains(@class,'${key_fragmentName}')]] | //div[contains(@style,'border-radius: ${key_borderRadius}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_BOX_SHADOW</td>
	<td>//div[contains(@style,'box-shadow: var(--${key_boxShadow})')][.//*[contains(@class,'${key_fragmentName}')]] | //div[contains(@style,'box-shadow: ${key_boxShadow}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_MARGIN_BOTTOM</td>
	<td>//div[contains(@class,'mb-${key_marginBottom}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_MARGIN_LEFT</td>
	<td>//div[contains(@class,'ml-${key_marginLeft}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_MARGIN_RIGHT</td>
	<td>//div[contains(@class,'mr-${key_marginRight}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
