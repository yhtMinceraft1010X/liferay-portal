</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_PADDING_BOTTOM</td>
	<td>//div[contains(@class,'pb-${key_paddingBottom}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_PADDING_LEFT</td>
	<td>//div[contains(@class,'pl-${key_paddingLeft}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_PADDING_RIGHT</td>
	<td>//div[contains(@class,'pr-${key_paddingRight}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
	<td></td>
</tr>
<tr>
	<td>FRAGMENT_CONFIGURATION_PADDING_TOP</td>
	<td>//div[contains(@class,'pt-${key_paddingTop}')][.//*[contains(@class,'${key_fragmentName}')]]</td>
