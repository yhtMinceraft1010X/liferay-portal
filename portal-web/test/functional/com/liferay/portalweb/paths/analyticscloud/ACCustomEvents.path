<html>
<head>
<title>ACCustomEvents</title>
</head>

<body>
<table border="1" cellpadding="1" cellspacing="1">
<thead>
<tr><td rowspan="1" colspan="3">ACCustomEvents</td></tr>
</thead>

<tbody>
<!-- BUTTON_TO_SEND_MANY_CUSTOM_EVENTS -->
<tr>
	<td>SEND_CUSTOM_EVENTS</td>
	<td>//button[@onclick]</td>
	<td></td>
</tr>
<!-- EVENT_LIST_COLUMN-->
<tr>
	<td>DISPLAY_NAME_OR_DESCRIPTION_LIST</td>
	<td>//td[contains(@class,'text-truncate')and text()='${key_nameValue}']</td>
	<td></td>
</tr>
<tr>
	<td>SAMPLE_RAW_DATA_LIST</td>
	<td>//th/ancestor::table//a[text()='${key_attributeName}']/ancestor::tr//td[contains(@class,'table-cell-expand-smaller')]</td>
	<td></td>
</tr>
<!-- SAMPLE_RAW_DATA_TABLE-->
<tr>
	<td>SAMPLE_RAW_DATA_IN_ORDER</td>
	<td>xpath=(//td[contains(@class,'table-cell-expand-small')])[${index}]</td>
	<td></td>
</tr>
<!-- DATA_TYPE -->
<tr>
	<td>DATA_TYPE_LIST</td>
	<td>//td[contains(@class,'text-truncate')and text()='${key_attributeName}']//following-sibling::td//div</td>
	<td></td>
</tr>
<!-- EDIT_CUSTOM_EVENT -->
<tr>
	<td>EDIT_MODAL_TITLE</td>
	<td>//*[@class='modal-title']</td>
	<td></td>
</tr>
<tr>
	<td>INVALID_MESSAGE</td>
	<td>//*[contains(@class,'form-feedback-item')]</td>
	<td></td>
</tr>
<!-- FILL_CUSTOM_EVENT_FIELDS -->
<tr>
	<td>INPUT_CUSTOM_EVENT</td>
	<td>//input[@id='${key_inputId}']</td>
	<td></td>
</tr>
<tr>
	<td>TEXTAREA_CUSTOM_EVENT</td>
	<td>//textarea[@id='${key_textareaId}']</td>
	<td></td>
</tr>
<tr>
	<td>INPUT_BUTTON</td>
	<td>//input[@type='submit']</td>
	<td></td>
</tr>
<!-- BLOCK_CUSTOM_EVENT -->
<tr>
	<td>BLOCK_CUSTOM_EVENT_LIST</td>
	<td>//a[contains(@href,'block-list')]</td>
	<td></td>
</tr>
<tr>
	<td>BLOCK_UNBLOCK_EVENT_BUTTON</td>
	<td>//*[text()='${key_nameList}']/ancestor::tr//div[@class='quick-action-menu']//button[contains(@alt,'${key_actionButton}')]</td>
	<td></td>
</tr>
<tr>
	<td>ALERT_LIMIT_REACHED</td>
	<td>//div[contains(@class,'alert-warning')]</td>
	<td></td>
</tr>
<tr>
	<td>BLOCK_LIST_EMPTY_MESSAGE</td>
	<td>//*[text()="To block events, select one from the events' table."]</td>
	<td></td>
</tr>
</tbody>
</table>
</body>
</html>