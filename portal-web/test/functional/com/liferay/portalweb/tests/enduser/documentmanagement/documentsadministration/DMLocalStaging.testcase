	@description = "This is a test for LPS-136828. It checks that multiple metadata sets can be published."
	@priority = "3"
	test CanPublishMetadataSets {
		DMNavigator.openMetadataSetsAdmin(siteURLKey = "site-name-staging");

		for (var n : list "1,2") {
			DMMetadataSet.addTitle(metadataSetName = "DM Metadata Set Name ${n}");

			DataEngine.addField(
				fieldFieldLabel = "Text",
				fieldName = "Text");

			DDLDataDefinition.save();
		}

		Navigator.gotoStagedSitePage(
			pageName = "Staging DM Page",
			siteName = "Site Name");

		Staging.gotoPublishToLive();

		Staging.publishToLive();

		DMNavigator.openMetadataSetsAdmin(siteURLKey = "site-name");

		for (var n : list "1,2") {
			DMMetadataSet.viewMetadataSetTitle(metadataSetTitle = "DM Metadata Set Name ${n}");
		}
	}

