		task ("View the changes of navigation menu not shown in Navigation Menu widget") {
			AssertVisible(
				key_pageName = "Test Page Name 1",
				locator1 = "Home#PAGE");

			AssertVisible(
				key_pageName = "Liferay",
				locator1 = "Home#PAGE");

			AssertElementNotPresent(
				key_pageName = "Test Page Name 2",
				locator1 = "Home#PAGE");
