		}

		task ("Add the second web content with White,Yellow,Red,Female") {
			JSONWebcontent.addWebContent(
				assetCategoryIds = "${whiteId},${yellowId},${redId},${femaleId}",
				content = "Levis 501",
				groupName = "Test Site Name",
