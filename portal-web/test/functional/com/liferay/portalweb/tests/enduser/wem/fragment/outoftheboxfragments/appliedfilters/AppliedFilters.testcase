		}

		task ("View applied category filter at Preview mode") {
			PageEditor.openPreviewMode();

			SelectFrame.selectFrameNoLoading(locator1 = "IFrame#MODAL_BODY");

			var n = "1";

			for (var style : list "510,501,511") {
