			wikiNodeName = "Main",
			wikiPageContent = "Wiki Page Content Edit",
			wikiPageName = "FrontPage");

		WikiNavigator.openToWikiPage(
			layoutURLKey = "wiki-test-page",
			siteURLKey = "guest",
			wikiNodeName = "Main",
			wikiPageTitle = "FrontPage");

		WikiPage.viewPG(
			wikiPageContent = "Wiki Page Content Edit",
			wikiPageTitle = "FrontPage");

		WikiPage.revertEditPG(
			wikiPageContent = "Wiki Page Content",
