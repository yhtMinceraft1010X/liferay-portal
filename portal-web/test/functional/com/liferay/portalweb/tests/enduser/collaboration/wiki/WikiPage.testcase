	@refactordone
	test CanRevertEditInFrontPage {
		JSONWiki.updateWikiPage(
			groupName = "Guest",
			wikiNodeName = "Main",
			wikiPageContent = "Wiki Page Content",
			wikiPageName = "FrontPage");

		JSONWiki.updateWikiPage(
