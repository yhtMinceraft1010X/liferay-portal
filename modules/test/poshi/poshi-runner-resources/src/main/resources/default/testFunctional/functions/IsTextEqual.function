@default = "isTextEqual"
definition {

	function isPartialText {
		selenium.waitForElementPresent();

		selenium.waitForVisible();

		selenium.isPartialTextCaseInsensitive();
	}

	function isText {
		selenium.waitForElementPresent();

		selenium.waitForVisible();

		selenium.isTextCaseInsensitive();
	}

	function isTextEqual {
		if (contains("${locator1}", "/input")) {
			IsTextEqual.isValue();
		}
		else {
			IsTextEqual.isText();
		}
	}

	function isValue {
		selenium.waitForElementPresent();

		selenium.waitForVisible();

		selenium.isValue();
	}

}