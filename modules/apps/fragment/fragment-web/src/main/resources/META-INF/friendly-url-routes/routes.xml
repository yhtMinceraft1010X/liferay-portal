<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">

<routes>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/fragment_entry/{fragmentEntryId:\d+}/edit</pattern>
		<ignored-parameter name="redirect" />
		<implicit-parameter name="mvcRenderCommandName">/fragment/edit_fragment_entry</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/fragment_entry/by_key/{fragmentEntryKey}/edit</pattern>
		<ignored-parameter name="redirect" />
		<implicit-parameter name="mvcRenderCommandName">/fragment/edit_fragment_entry</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/fragment_entry/{fragmentEntryId:\d+}/group_usages</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/view_group_fragment_entry_usages</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/fragment_entry/{fragmentEntryId:\d+}/usages</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/view_fragment_entry_usages</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/fragment_entry/{fragmentEntryId:\d+}/render</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/render_fragment_entry</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/edit</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/edit_fragment_collection</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/new</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/edit_fragment_collection</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/select</pattern>
		<implicit-parameter name="mvcRenderCommandName">/fragment/select_fragment_collection</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}/view_resources</pattern>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
		<implicit-parameter name="viewResources">true</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionId:\d+}</pattern>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collection/{fragmentCollectionKey}</pattern>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern>/fragment_collections</pattern>
		<ignored-parameter name="mvcRenderCommandName" />
		<ignored-parameter name="p_v_l_s_g_id" />
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
	<route>
		<pattern></pattern>
		<ignored-parameter name="mvcRenderCommandName" />
		<ignored-parameter name="p_v_l_s_g_id" />
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
</routes>