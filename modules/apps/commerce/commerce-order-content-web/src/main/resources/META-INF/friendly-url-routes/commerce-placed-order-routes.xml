<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">

<routes>
	<route>
		<pattern>/{commerceOrderId:\d+}</pattern>
		<implicit-parameter name="itemsPerPage">10</implicit-parameter>
		<implicit-parameter name="mvcRenderCommandName">/commerce_order_content/view_commerce_order_details</implicit-parameter>
		<implicit-parameter name="p_p_id">com_liferay_commerce_order_content_web_internal_portlet_CommerceOrderContentPortlet</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="pageNumber">1</implicit-parameter>
		<implicit-parameter name="tableName">commercePlacedOrders</implicit-parameter>
	</route>
</routes>