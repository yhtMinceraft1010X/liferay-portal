			column = "1",
			groupName = "Second Site",
			layoutName = "Wiki Page",
			widgetName = "Wiki");

		Navigator.openSiteURL(siteName = "Second Site");

		LAR.importPortlet(
			larFileName = "Wiki.portlet.lar",
			wikiPortlet = "true");

