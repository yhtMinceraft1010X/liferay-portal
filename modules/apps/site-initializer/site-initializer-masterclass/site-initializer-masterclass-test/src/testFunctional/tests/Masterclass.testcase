			StyleBooksAdmin.gotoStyleBook(styleBookName = "Masterclass Classic");
		}

		task ("Click more to select a page for preview") {
			Click(
				index = "1",
				locator1 = "StyleBookEditor#PREVIEW_SELECTOR");
		}

		task ("Assert private pages is not shown") {
