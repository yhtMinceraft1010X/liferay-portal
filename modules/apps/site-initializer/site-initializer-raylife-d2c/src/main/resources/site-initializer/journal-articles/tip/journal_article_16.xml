}

#tip {
	width: 276px;
}
</style>
<script>
const quoteComparsionList = document.querySelector(".quote-comparsion-list");

function _addItemToList(text) {
	var li = document.createElement("li");
	li.appendChild(document.createTextNode(text));
	quoteComparsionList.appendChild(li);
}

const raylifeApplicationForm = JSON.parse(localStorage.getItem("raylife-application-form"));
const address = raylifeApplicationForm?.basics?.businessInformation?.business?.location;
const productCategory = raylifeApplicationForm?.basics?.productCategory;
const annualRevenue = raylifeApplicationForm?.employees?.estimatedAnnualGrossRevenue;
const partTimeEmployees = raylifeApplicationForm?.employees?.partTimeEmployees;

_addItemToList(productCategory);
_addItemToList(`${annualRevenue} annual revenue`);
_addItemToList(`${address.address}, ${address.city} ${address.state} ${address.zip}`);
_addItemToList(`${partTimeEmployees} Total Employees, 1 Owner`);

</script>]]></dynamic-content>
