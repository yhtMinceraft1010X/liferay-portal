@component-name = "data-engine"
definition {

	property portal.release = "true";
	property portal.upstream = "true";
	property testray.main.component.name = "Data Engine";

	setUp {
		TestCase.setUpPortalInstance();

		User.firstLoginPG();

		JSONGroup.addGroup(groupName = "Test Site Name");
	}

	tearDown {
		var testPortalInstance = PropsUtil.get("test.portal.instance");

		if ("${testPortalInstance}" == "true") {
			PortalInstances.tearDownCP();
		}
		else {
			JSONGroup.deleteGroupByName(groupName = "Test Site Name");
		}
	}

	@description = "Verify that is possible to translate Web Content fields with different Instance language set"
	@priority = "4"
	test CanBeTranslatedWithDifferentInstanceLanguages {
		property test.name.skip.portal.instance = "DELocalization#CanBeTranslatedWithDifferentInstanceLanguages";

		ApplicationsMenu.gotoPortlet(
			category = "Configuration",
			panel = "Control Panel",
			portlet = "Instance Settings");

		SystemSettings.gotoConfiguration(
			configurationCategory = "Localization",
			configurationName = "Language",
			configurationScope = "Virtual Instance Scope");

		PortalSettings.configureCurrentLanguagesCP(defaultPortalLanguage = "Portuguese (Brazil)");

		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");

		WebContentNavigator.gotoAddCP();

		WebContent.addCP(
			webContentContent = "WC WebContent Content",
			webContentTitle = "WC WebContent Title");

		PortletEntry.publish();

		WebContentNavigator.gotoEditCP(webContentTitle = "WC WebContent Title");

		WebContent.editCP(
			webContentContentEdit = "WC WebContent Content pt-BR",
			webContentTitle = "WC WebContent Title",
			webContentTitleEdit = "WC WebContent Title pt-BR");

		WebContentNavigator.gotoEditCP(webContentTitle = "WC WebContent Title pt-BR");

		PortletEntry.changeLocale(locale = "en-US");

		WebContent.editWebContentContent(
			webContentContentEdit = "WC WebContent Content en-US",
			webContentTitle = "WC WebContent Title pt-BR",
			webContentTitleEdit = "WC WebContent Title en-US");

		WebContent.viewTitle(webContentTitle = "WC WebContent Title en-US");

		WebContentNavigator.gotoEditCP(webContentTitle = "WC WebContent Title en-US");

		WebContent.viewCP(
			webContentContent = "WC WebContent Content pt-BR",
			webContentTitle = "WC WebContent Title pt-BR");

		PortletEntry.changeLocale(locale = "en-US");

		WebContent.viewCP(
			webContentContent = "WC WebContent Content en-US",
			webContentTitle = "WC WebContent Title en-US");

		ApplicationsMenu.gotoPortlet(
			category = "Configuration",
			panel = "Control Panel",
			portlet = "Instance Settings");

		SystemSettings.gotoConfiguration(
			configurationCategory = "Localization",
			configurationName = "Language",
			configurationScope = "Virtual Instance Scope");

		PortalSettings.configureCurrentLanguagesCP(defaultPortalLanguage = "English (United States)");
	}

	@description = "Verify that is possible to choose different options for Grid field for a translated language"
	@priority = "5"
	test CanChooseDifferentOptionsFromGridField {
		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");

		NavItem.gotoStructures();

		WebContentStructures.addCP(
			structureDescription = "Structure Description Test",
			structureName = "Structure Name Test");

		DataEngine.addField(
			fieldFieldLabel = "Grid",
			fieldName = "Grid");

		DEBuilder.addFieldOptions(
			fieldLabel = "Grid",
			optionFieldLabel = "Rows",
			optionValueList = "Option 1,Option 2,Option 3, Option 4, Option 5, Option 6");

		DEBuilder.addFieldOptions(
			fieldLabel = "Grid",
			optionFieldLabel = "Columns",
			optionValueList = "Option 1,Option 2,Option 3, Option 4, Option 5, Option 6");

		WebContentStructures.addTranslations(localeList = "zh-CN");

		PortletEntry.changeLocale(locale = "zh-CN");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Grid",
			fieldFieldLabelEdit = "网格");

		WebContentStructures.saveCP();

		NavItem.click(navItem = "Web Content");

		WebContentNavigator.gotoAddWithStructureCP(structureName = "Structure Name Test");

		PortletEntry.inputTitle(title = "Test");

		for (var index : list "1,2,3") {
			DERenderer.inputDataInGridField(
				column = "${index}",
				fieldLabel = "Grid",
				row = "${index}");
		}

		PortletEntry.changeLocale(locale = "zh-CN");

		PortletEntry.inputTitle(title = "测试");

		for (var index : list "4,5,6") {
			DERenderer.inputDataInGridField(
				column = "${index}",
				fieldLabel = "网格",
				row = "${index}");
		}

		PortletEntry.publish();

		WebContentNavigator.gotoEditCP(webContentTitle = "Test");

		AssertTextEquals(
			locator1 = "TextInput#TITLE",
			value1 = "Test");

		for (var index : list "1,2,3") {
			DERenderer.assertDataInGridField(
				column = "${index}",
				fieldLabel = "Grid",
				row = "${index}");
		}

		PortletEntry.changeLocale(locale = "zh-CN");

		AssertTextEquals(
			locator1 = "TextInput#TITLE",
			value1 = "测试");

		for (var index : list "1,2,3,4,5,6") {
			DERenderer.assertDataInGridField(
				column = "${index}",
				fieldLabel = "网格",
				row = "${index}");
		}
	}

	@description = "Verify that is possible to choose different options for Multiple Selection field for a translated language"
	@priority = "5"
	test CanChooseDifferentOptionsFromMultpleSelectionField {
		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");

		NavItem.gotoStructures();

		WebContentStructures.addCP(
			structureDescription = "Structure Description Test",
			structureName = "Structure Name Test");

		DEBuilder.addField(
			fieldLabel = "Multiple Selection",
			fieldName = "Multiple Selection");

		DEBuilder.addFieldOptions(
			fieldLabel = "Multiple Selection",
			optionValueList = "Option 1,Option 2, Option 3, Option 4");

		WebContentStructures.addTranslations(localeList = "zh-CN");

		PortletEntry.changeLocale(locale = "zh-CN");

		DataEngine.editFieldLabel(
			fieldFieldLabel = "Multiple Selection",
			fieldFieldLabelEdit = "复选列表框");

		WebContentStructures.saveCP();

		NavItem.click(navItem = "Web Content");

		WebContentNavigator.gotoAddWithStructureCP(structureName = "Structure Name Test");

		PortletEntry.inputTitle(title = "Test");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 1");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 2");

		PortletEntry.changeLocale(locale = "zh-CN");

		PortletEntry.inputTitle(title = "测试");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 1");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 2");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 3");

		DERenderer.inputDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 4");

		PortletEntry.publish();

		WebContentNavigator.gotoEditCP(webContentTitle = "Test");

		AssertTextEquals(
			locator1 = "TextInput#TITLE",
			value1 = "Test");

		DERenderer.assertDataInMultipleSelectionField(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 1");

		DERenderer.assertDataInMultipleSelectionField(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 2");

		DERenderer.assertMultipleSelectionOptionIsUnchecked(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 3");

		DERenderer.assertMultipleSelectionOptionIsUnchecked(
			fieldLabel = "Multiple Selection",
			optionValue = "Option 4");

		PortletEntry.changeLocale(locale = "zh-CN");

		AssertTextEquals(
			locator1 = "TextInput#TITLE",
			value1 = "测试");

		DERenderer.assertDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 3");

		DERenderer.assertDataInMultipleSelectionField(
			fieldLabel = "复选列表框",
			optionValue = "Option 4");

		DERenderer.assertMultipleSelectionOptionIsUnchecked(
			fieldLabel = "复选列表框",
			optionValue = "Option 1");

		DERenderer.assertMultipleSelectionOptionIsUnchecked(
			fieldLabel = "复选列表框",
			optionValue = "Option 2");
	}

