	@description = "Verify that the Date field mask is succesfully applied for languages which uses DD/MM/YYYY"
	@priority = "5"
	test CanDateMaskBeAppliedForTranslations {
		WebContentNavigator.openWebContentAdmin(siteURLKey = "test-site-name");

		NavItem.gotoStructures();

		WebContentStructures.addCP(
			structureDescription = "WC Structure Description",
			structureName = "WC Structure Title");

		DataEngine.addField(
			fieldFieldLabel = "Date",
			fieldName = "Date");

		WebContentStructures.addTranslations(localeList = "pt-BR,zh-CN");

		for (var language : list "pt-BR,zh-CN") {
			PortletEntry.changeLocale(locale = "${language}");

			WebContentStructures.addName(structureName = "WC Structure Title - ${language}");

			DataEngine.editFieldLabel(
				fieldFieldLabel = "Date",
				fieldFieldLabelEdit = "Date - ${language}");
		}

		WebContentStructures.saveCP();

		NavItem.gotoWebContent();

		WebContentNavigator.gotoAddWithStructureCP(structureName = "WC Structure Title");

		WebContent.addWithStructureCP(
			webContentDate = "01/02/3000",
			webContentTitle = "WC Structure Title");

		PortletEntry.changeLocale(locale = "pt-BR");

		AssertTextEquals.assertValue(
			key_fieldFieldLabel = "Date - pt-BR",
			locator1 = "WCEditWebContent#DATE_INPUT",
			value1 = "02/01/3000");

		PortletEntry.changeLocale(locale = "zh-CN");

		AssertTextEquals.assertValue(
			key_fieldFieldLabel = "Date - zh-CN",
			locator1 = "WCEditWebContent#DATE_INPUT",
			value1 = "3000/01/02");
	}

