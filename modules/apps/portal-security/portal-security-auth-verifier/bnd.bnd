Bundle-Name: Liferay Portal Security Auth Verifier
Bundle-SymbolicName: com.liferay.portal.security.auth.verifier
Bundle-Version: 6.0.13
Require-Capability:\
	osgi.extender;\
		filter := "(&(osgi.extender=osgi.configurator)(version>=1.0)(!(version>=2.0)))"
-dsannotations-options: inherit
-includeresource: OSGI-INF/configurator=configurator
-metatype-inherit: true