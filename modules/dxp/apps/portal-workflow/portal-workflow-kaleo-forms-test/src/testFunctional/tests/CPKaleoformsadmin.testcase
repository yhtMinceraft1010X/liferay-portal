	@priority = "5"
	test DeactivateAndActivateWorkflowDefinition {
		property portal.acceptance = "true";
		property test.name.skip.portal.instance = "CPKaleoformsadmin#DeactivateAndActivateWorkflowDefinition";

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Kaleo Forms Admin");

		KaleoFormsAdmin.addFieldSet(
			kfFieldSetName = "New Kaleo Field Set",
			kfProcessName = "Kaleo Forms Process");

		DynamicDataMapping.addField(
			field = "Text",
			fieldFieldLabel = "Text",
			fieldName = "Text");

		KaleoFormsAdmin.saveFieldSet();

		KaleoFormsAdmin.addProcess();

		KaleoFormsAdmin.addProcessDetails(
			kfProcessDescription = "Kaleo Forms Process Description",
			kfProcessName = "Kaleo Forms Process");

		KaleoFormsAdmin.next();

		KaleoFormsAdmin.chooseFieldSet(
			kfFieldSetName = "New Kaleo Field Set",
			kfProcessName = "Kaleo Forms Process");

		KaleoFormsAdmin.next();

		KaleoFormsAdmin.deactivateWorkflow(workflowDefinitionTitle = "Single Approver");

		KaleoFormsAdmin.viewNoPublishedWorkflowDefinitions();

		KaleoFormsAdmin.viewUnpublishedWorkflowDefinitions();

		AssertTextEquals(
			key_workflowDefinitionTitle = "Single Approver",
			locator1 = "KaleoFormsAdminEditProcess#WORKFLOW_TABLE_TITLE",
			value1 = "Single Approver");

		KaleoFormsAdmin.reactivateInactiveWorkflowDefinition(workflowDefinitionTitle = "Single Approver");

		KaleoFormsAdmin.viewPublishedWorkflowDefinitions();

		AssertTextEquals(
			key_workflowDefinitionTitle = "Single Approver",
			locator1 = "KaleoFormsAdminEditProcess#WORKFLOW_TABLE_TITLE",
			value1 = "Single Approver");

		KaleoFormsAdmin.viewNoUnpublishedWorkflowDefinitions();
	}

	@priority = "5"
	test DeactivateAndDeleteWorkflowDefinition {
		property portal.acceptance = "true";
		property test.name.skip.portal.instance = "CPKaleoformsadmin#DeactivateAndDeleteWorkflowDefinition";

		Navigator.openURL();

		ProductMenu.gotoPortlet(
			category = "Content & Data",
			portlet = "Kaleo Forms Admin");

		KaleoFormsAdmin.addFieldSet(
			kfFieldSetName = "New Kaleo Field Set",
			kfProcessName = "Kaleo Forms Process");

		DynamicDataMapping.addField(
			field = "Text",
			fieldFieldLabel = "Text",
			fieldName = "Text");

		KaleoFormsAdmin.saveFieldSet();

		KaleoFormsAdmin.addProcess();

		KaleoFormsAdmin.addProcessDetails(
			kfProcessDescription = "Kaleo Forms Process Description",
			kfProcessName = "Kaleo Forms Process");

		KaleoFormsAdmin.next();

		KaleoFormsAdmin.chooseFieldSet(
			kfFieldSetName = "New Kaleo Field Set",
			kfProcessName = "Kaleo Forms Process");

		KaleoFormsAdmin.next();

		KaleoFormsAdmin.addWorkflow(
			kfProcessName = "Kaleo Forms Process",
			workflowDefinitionTitle = "Workflow Definition Title");

		KaleoDesigner.addSourceXMLBasicDefinition();

		KaleoFormsAdmin.publishWorkflow(workflowDefinitionTitle = "Workflow Definition Title");

		for (var workflowDefinitionTitle : list "Single Approver,Workflow Definition Title") {
			KaleoFormsAdmin.deactivateWorkflow(workflowDefinitionTitle = "${workflowDefinitionTitle}");
		}

		KaleoFormsAdmin.viewNoPublishedWorkflowDefinitions();

		KaleoFormsAdmin.viewUnpublishedWorkflowDefinitions();

		for (var workflowDefinitionTitle : list "Single Approver,Workflow Definition Title") {
			AssertTextEquals(
				key_workflowDefinitionTitle = "${workflowDefinitionTitle}",
				locator1 = "KaleoFormsAdminEditProcess#WORKFLOW_TABLE_TITLE",
				value1 = "${workflowDefinitionTitle}");
		}

		KaleoFormsAdmin.deleteWorkflowDraft(workflowDefinitionTitle = "Workflow Definition Title");

		KaleoFormsAdmin.reactivateInactiveWorkflowDefinition(workflowDefinitionTitle = "Single Approver");

		KaleoFormsAdmin.viewPublishedWorkflowDefinitions();

		AssertTextEquals(
			key_workflowDefinitionTitle = "Single Approver",
			locator1 = "KaleoFormsAdminEditProcess#WORKFLOW_TABLE_TITLE",
			value1 = "Single Approver");

		KaleoFormsAdmin.viewNoUnpublishedWorkflowDefinitions();
	}

